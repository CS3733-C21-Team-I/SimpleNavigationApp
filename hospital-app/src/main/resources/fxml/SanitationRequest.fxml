         <graphic>
            <ImageView fitHeight="49.0" fitWidth="49.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@fxmlResources/baseline_arrow_back_ios_white_18dp.png" />
               </image>
            </ImageView>
         </graphic>
