                  <JFXTextField fx:id="addressSearch" focusColor="#ffffff00" onInputMethodTextChanged="#autoComplete" onKeyReleased="#autoComplete" prefHeight="80.0" prefWidth="500.0" promptText="Starting address" style="-fx-background-color: WHITE; -fx-border-color: #0067b1; -fx-border-radius: 40; -fx-border-width: 3; -fx-background-radius: 40; -jfx-unfocus-color: TRANSPARENT;">
