                  </JFXTextField>
                  <JFXButton fx:id="submitButton" buttonType="RAISED" ellipsisString="" onAction="#submit" prefHeight="80.0" prefWidth="80.0" ripplerFill="WHITE" style="-fx-background-color: #0067b1; -fx-background-radius: 40;" text="GO" textFill="WHITE" textOverrun="CLIP">
