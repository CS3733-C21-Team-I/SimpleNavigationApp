            </JFXListView>
            <VBox VBox.vgrow="ALWAYS" />
            <JFXButton buttonType="RAISED" onAction="#switchWindow" prefHeight="80.0" prefWidth="370.0" ripplerFill="WHITE" style="-fx-background-color: #012D5A; -fx-background-radius: 40;" text="I've Arrived At The Hospital" textFill="WHITE">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
