<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c21.teamI.user.view.EmployeeTableController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXListView fx:id="roleList" prefHeight="500.0" prefWidth="800.0">

            </JFXListView>
            <HBox alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="800.0">
               <children>
                  <TextField fx:id="employeeID" prefHeight="30.0" prefWidth="140.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="14.0" right="9.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="firstName" prefWidth="140.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="9.0" right="9.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="lastName" prefWidth="140.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="9.0" right="9.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="gender" prefWidth="140.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="9.0" right="9.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="position" prefWidth="140.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="9.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="800.0">
               <children>
                  <Button fx:id="addBtn" mnemonicParsing="false" text="Add">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="delBtn" mnemonicParsing="false" text="Delete">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="60.0" left="60.0" right="60.0" top="60.0" />
   </padding>
</AnchorPane>
