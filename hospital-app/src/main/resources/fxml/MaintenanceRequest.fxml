<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="973.0" xmlns="http://javafx.com/javafx/15.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.c21.teamI.hospitalMap.mapEditing.ApplicationView">
   <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#0067b1" height="105.0" layoutY="8.0" stroke="BLACK"
              strokeType="INSIDE" width="973.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0"/>
   <Label alignment="CENTER" layoutX="184.0" layoutY="16.0" text="Maintenance Request Form" textFill="WHITE"
          AnchorPane.bottomAnchor="711.0" AnchorPane.leftAnchor="184.0" AnchorPane.rightAnchor="184.0"
          AnchorPane.topAnchor="16.0">
      <font>
         <Font size="50.0"/>
      </font>
   </Label>
   <Button fx:id="maintenanceReturn" layoutX="14.0" layoutY="122.0" mnemonicParsing="false" onAction="#navigate"
           styleClass="styledButton" stylesheets="@fxmlResources/main.css" text="Back">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Button>
   <VBox layoutX="437.0" layoutY="122.0" prefHeight="678.0" prefWidth="573.0" AnchorPane.bottomAnchor="0.0"
         AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="122.0">
      <HBox prefHeight="55.0" prefWidth="573.0">
         <Label text="Maintenance Request Type:">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <MenuButton mnemonicParsing="false" prefHeight="25.0" prefWidth="164.0" text="Select...">
            <items>
               <MenuItem mnemonicParsing="false" text="Elevator Malfunction"/>
               <MenuItem mnemonicParsing="false" text="Hospital Bed"/>
               <MenuItem mnemonicParsing="false" text="Automatic Door Malfunction"/>
               <MenuItem mnemonicParsing="false" text="Medical Device Repair"/>
               <MenuItem mnemonicParsing="false" text="Lightbulb Replacement"/>
               <MenuItem mnemonicParsing="false" text="Other"/>
            </items>
            <HBox.margin>
               <Insets top="10.0"/>
            </HBox.margin>
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
         </MenuButton>
      </HBox>
      <HBox prefHeight="123.0" prefWidth="573.0">
         <Label text="Request Details:">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <TextArea prefHeight="85.0" prefWidth="434.0" promptText="Please specify any details about your request">
            <HBox.margin>
               <Insets bottom="10.0" top="10.0"/>
            </HBox.margin>
         </TextArea>
      </HBox>
      <HBox prefHeight="48.0" prefWidth="573.0">
         <Label prefHeight="26.0" prefWidth="165.0" text="Location:">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <TextField prefHeight="33.0" prefWidth="479.0" promptText="Request location">
            <HBox.margin>
               <Insets top="10.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox prefHeight="52.0" prefWidth="573.0">
         <Label prefHeight="26.0" prefWidth="210.0" text="Employee Contact:">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <TextField prefHeight="33.0" prefWidth="474.0" promptText="Employee contact for request">
            <HBox.margin>
               <Insets top="10.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox prefHeight="82.0" prefWidth="573.0">
         <Label prefHeight="26.0" prefWidth="145.0" text="Emergency Priority:">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <CheckBox mnemonicParsing="false" prefHeight="18.0" prefWidth="17.0">
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
            <HBox.margin>
               <Insets top="15.0"/>
            </HBox.margin>
         </CheckBox>
      </HBox>
      <HBox prefHeight="82.0" prefWidth="573.0">
         <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="120.0" styleClass="styledButton"
                 stylesheets="@fxmlResources/main.css" text="Submit">
            <HBox.margin>
               <Insets left="20.0" right="10.0"/>
            </HBox.margin>
            <graphic>
               <ImageView fitHeight="53.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@fxmlResources/baseline_send_white_18dp.png"/>
               </ImageView>
            </graphic>
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
         </Button>
         <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="107.0" styleClass="styledButton"
                 stylesheets="@fxmlResources/main.css" text="Clear">
            <HBox.margin>
               <Insets right="10.0"/>
            </HBox.margin>
            <graphic>
               <ImageView fitHeight="25.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@fxmlResources/baseline_delete_white_18dp.png"/>
               </ImageView>
            </graphic>
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
         </Button>
         <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="107.0" styleClass="styledButton"
                 stylesheets="@fxmlResources/main.css" text="Cancel">
            <graphic>
               <ImageView fitHeight="26.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@fxmlResources/baseline_close_white_18dp.png"/>
               </ImageView>
            </graphic>
            <cursor>
               <Cursor fx:constant="HAND"/>
            </cursor>
         </Button>
      </HBox>
   </VBox>
</AnchorPane>
